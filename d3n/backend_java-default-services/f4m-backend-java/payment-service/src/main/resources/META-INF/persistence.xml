<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd" version="1.0">

<persistence-unit name="db-manager">
    <class>de.ascendro.f4m.service.payment.payment.system.model.AccountBalance</class>
    <class>de.ascendro.f4m.service.payment.payment.system.model.Currencies</class>
    <class>de.ascendro.f4m.service.payment.payment.system.model.TenantStatus</class>
    <class>de.ascendro.f4m.service.payment.payment.system.model.Tenant</class>
    <class>de.ascendro.f4m.service.payment.payment.system.model.GameBalance</class>
    <class>de.ascendro.f4m.service.payment.payment.system.model.Transaction</class>
    <class>de.ascendro.f4m.service.payment.payment.system.model.TenantBalance</class>



    <properties>
        <!-- Disable the second-level cache  -->
        <property name="hibernate.cache.provider_class" value="org.hibernate.cache.NoCacheProvider"/>

        <!-- Default is false for backwards compatibility.  Should be used on all new projects -->
        <!--<property name="hibernate.id.new_generator_mappings" value="true"/>-->
    </properties>
</persistence-unit>

</persistence>
